#!/usr/bin/env python
# -*- encoding: utf-8 -*-
##############################################################################
#
##############################################################################

from openerp.osv import osv, fields

class product_template(osv.osv):
    _inherit = 'product.template'

    _columns = {
        'unique_name': fields.char('Unique Name', size=128, required=True, translate=True),
    }

product_template()

