<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="sequence_contract" model="ir.sequence">
            <field name="name">Contracten</field>
            <field name="code">account.analytic.account</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_project" model="ir.sequence">
            <field name="name">Projecten</field>
            <field name="code">account.analytic.account</field>
            <field name="prefix">P/%(year)s</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>
</openerp>
